{
  "name": "airtable-importer",
  "version": "1.0.0",
  "description": "A Vite React Electron app for importing data to Airtable",
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "build:electron": "npm run build && electron-builder",
    "electron": "electron ."
  },
  "keywords": [
    "electron",
    "react",
    "vite",
    "airtable",
    "importer"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "vite": "^5.0.8",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "airtable": "^0.12.2",
    "iconv-lite": "^0.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xml2js": "^0.6.2"
  },
  "build": {
    "appId": "com.airtable.importer",
    "productName": "Airtable Importer",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "node_modules/**/*"
    ]
  }
}
